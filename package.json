{
  "name": "webpack-5-frontend-starter",
  "version": "1.3.0",
  "description": "Универсальная стартовая frontend сборка Webpack 5 (VUE REACT SCSS PUG TWIG HTML) для одностраничного или многостраничного сайта.",
  "keywords": [
    "webpack",
    "frontend",
    "layout",
    "multi-page",
    "multiple",
    "pug",
    "twig",
    "html",
    "bem",
    "scss",
    "vue",
    "react",
    "pug-bem",
    "vue-bem",
    "start",
    "components"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/uvarov-frontend/webpack-5-frontend-starter.git"
  },
  "author": {
    "name": "Yuri Uvarov",
    "email": "uvarov.frontend@gmail.com",
    "url": "https://frontend.uvarov.tech"
  },
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "start": "node ./utils/entry && cross-env NODE_ENV=development TEMP=true webpack serve --config ./config/webpack.dev.js",
    "dev": "node ./utils/entry && cross-env NODE_ENV=development TEMP=true webpack --config ./config/webpack.dev.js",
    "prod": "node ./utils/entry && cross-env NODE_ENV=production TEMP=true webpack --config ./config/webpack.prod.js",
    "utils:analyzer": "webpack-bundle-analyzer --port 4200 build/stats.json",
    "utils:ftp": "node ./utils/ftp",
    "utils:clean": "node ./utils/clean",
    "utils:entry": "node ./utils/entry"
  },
  "browserslist": [
    "> 1%",
    "last 3 version"
  ],
  "dependencies": {
    "normalize.css": "^8.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@nurminen/html-beautify-webpack-plugin": "^1.1.3",
    "@uvarov.frontend/pug-bem-plain-loader": "^1.1.6",
    "@vue/compiler-sfc": "^3.2.33",
    "autoprefixer": "^10.4.4",
    "babel-loader": "^8.2.4",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "compression-webpack-plugin": "^9.2.0",
    "copy-webpack-plugin": "^10.2.4",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "cssnano": "^5.1.7",
    "eslint": "^8.13.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-vue": "^8.6.0",
    "ftp-deploy": "^2.4.2",
    "glob-import-loader": "^1.2.0",
    "html-loader": "^3.1.0",
    "html-replace-webpack-plugin": "^2.6.0",
    "html-webpack-plugin": "^5.5.0",
    "image-webpack-loader": "^8.1.0",
    "mini-css-extract-plugin": "^2.6.0",
    "perfectionist": "^2.4.0",
    "postcss": "^8.4.12",
    "postcss-loader": "^6.2.1",
