{
	"name": "webpack-5-frontend-starter",
	"version": "1.3.3",
	"description": "Webpack 5 universal starter frontend build (VUE REACT SCSS PUG TWIG HTML) for a single page or multipage site.",
	"homepage": "https://frontend.uvarov.tech",
	"keywords": [
		"webpack",
		"frontend",
		"layout",
		"multi-page",
		"multiple",
		"pug",
		"twig",
		"html",
		"bem",
		"scss",
		"vue",
		"react",
		"pug-bem",
		"vue-bem",
		"start",
		"components"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/uvarov-frontend/webpack-5-frontend-starter.git"
	},
	"author": {
		"name": "Yuri Uvarov",
		"email": "uvarov.frontend@gmail.com",
		"url": "https://frontend.uvarov.tech"
	},
	"license": "MIT",
	"main": "./config/webpack.common.js",
	"scripts": {
		"utils:npm": "webpack --config ./utils/npm.js && rm ./npm/build/module-name.min.min.js",
		"utils:ftp": "node ./utils/ftp",
		"utils:clean": "node ./utils/clean",
		"utils:entry": "node ./utils/entry",
		"temp": "node ./config/temp.js TEMP=true",
		"start": "npm run utils:entry --silent && npm run temp --silent && cross-env NODE_ENV=development webpack serve --config ./config/webpack.dev.js",
		"dev": "npm run utils:entry --silent && npm run temp --silent && cross-env NODE_ENV=development webpack --config ./config/webpack.dev.js",
		"prod": "npm run utils:entry --silent && npm run temp --silent && cross-env NODE_ENV=production webpack --config ./config/webpack.prod.js"
	},
	"browserslist": [
		"> 1%",
		"last 3 version"
	],
	"dependencies": {
		"normalize.css": "^8.0.1"
	},
	"devDependencies": {
		"@babel/core": "^7.18.6",
		"@babel/plugin-syntax-dynamic-import": "^7.8.3",
		"@babel/plugin-transform-runtime": "^7.18.6",
		"@babel/preset-env": "^7.18.6",
		"@babel/preset-react": "^7.18.6",
		"@babel/runtime": "^7.18.6",
