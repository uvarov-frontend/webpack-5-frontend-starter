		"perfectionist": "^2.4.0",
		"postcss": "^8.4.14",
		"postcss-loader": "^7.0.0",
		"postcss-preset-env": "^7.7.2",
		"pug": "^3.0.2",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"readdir": "^1.0.2",
		"sass": "^1.53.0",
		"sass-loader": "^13.0.2",
		"stylelint": "^14.9.1",
		"stylelint-config-prettier": "^9.0.3",
