{
  "name": "webpack-5-frontend-starter",
  "version": "1.0.0",
  "description": "Универсальная стартовая frontend сборка Webpack 5 (VUE SCSS PUG TWIG HTML) для одностраничного или многостраничного сайта.",
  "keywords": [
    "webpack",
    "frontend",
    "layout",
    "multi-page",
    "multiple",
    "pug",
    "twig",
    "html",
    "bem",
    "scss",
    "vue",
    "pug-bem",
    "vue-bem",
    "start",
    "components"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/uvarov-frontend/webpack-5-frontend-starter.git"
  },
  "author": {
    "name": "Yuri Uvarov",
    "email": "uvarov.frontend@gmail.com",
    "url": "https://frontend.uvarov.tech"
  },
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --progress --config ./config/webpack.dev.js",
    "dev": "cross-env NODE_ENV=development webpack --config ./config/webpack.dev.js",
    "prod": "cross-env NODE_ENV=production webpack --config ./config/webpack.prod.js",
    "analyzer": "webpack-bundle-analyzer --port 4200 build/stats.json",
    "ftp": "node ./utils/ftp",
    "clear": "node ./utils/clear"
  },
  "browserslist": [
    "> 1%",
    "last 3 version"
  ],
  "dependencies": {
    "normalize.css": "^8.0.1"
  },
